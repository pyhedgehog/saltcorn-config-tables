{
  "name": "saltcorn-config-tables",
  "version": "0.3.0",
  "description": "Saltcorn plugin: Collection of external tables re-formatted from configuration",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "test": "echo No tests defined.;false"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.rutube.ru:mdubner/saltcorn-config-tables.git"
  },
  "keywords": [
    "saltcorn"
  ],
  "author": "Michael P. Dubner",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@saltcorn/admin-models": "0.9.5",
    "@saltcorn/data": "0.9.5",
    "@saltcorn/db-common": "0.9.5"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "plugin:n/recommended",
      "prettier"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": 9
    },
    "rules": {
      "n/no-unpublished-require": [
        "error",
        {
          "allowModules": [
            "@saltcorn/data"
          ]
        }
      ],
      "n/no-missing-require": [
        "error",
        {
          "allowModules": [
            "@saltcorn/data"
          ]
        }
      ]
    }
  }
}
